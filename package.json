{
  "name": "@jbs/vue3-formulate",
  "version": "2.5.3",
  "description": "A form of Vue Formulate for Vue 3 support and vite",
  "main": "dist/formulate.cjs.js",
  "module": "dist/formulate.es.js",
  "unpkg": "dist/formulate.iife.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },

  "scripts": {
    "build": "vite build & npm run build:css & wait && echo \"Build complete:\nesm: $(gzip -c dist/formulate.esm.js | wc -c)b gzip\numd: $(gzip -c dist/formulate.umd.js | wc -c)b gzip\nmin: $(gzip -c dist/formulate.min.js | wc -c)b gzip\"",
    "build:css": "sass src/themes/snow/snow.scss dist/snow.css && postcss --use autoprefixer -b '> 2%' dist/snow.css | postcss --no-map --use cssnano > dist/snow.min.css",
    "test": "cross-env NODE_ENV=test jest --config test/jest.conf.js --coverage",
    "test:watch": "NODE_ENV=test jest --config test/jest.conf.js --watch",
    "test:coverage": "NODE_ENV=test jest --config test/jest.conf.js --coverage",
    "cypress": "cypress open",
    "cypress:ci": "./build/cypress.sh",
    "build:size": "gzip -c dist/formulate.esm.js | wc -c",
    "dev": "vue-cli-service serve --port=7872 examples/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wearebraid/vue-formulate.git"
  },
  "keywords": [
    "vue",
    "form",
    "forms",
    "validation",
    "vuex",
    "validate"
  ],
  "author": "Justin Schroeder <justin@wearebraid.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wearebraid/vue-formulate/issues"
  },
  "homepage": "https://www.vueformulate.com",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.5",
    "cypress": "^13.14.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.9.1",
    "flush-promises": "^1.0.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.8",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vue": "^3.4.38",
    "wait-on": "^8.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@braid/vue-formulate-i18n": "^1.16.0",
    "is-plain-object": "^5.0.0"
  }
}
